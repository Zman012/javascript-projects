let milesToMoon = distanceToTheMoonKM * milesPerKilometer;
let hoursToMoon = milesToMoon / shuttleSpeedMPH;
let daysToMoon = hoursToMoon / 24;

console.log(nameOfSpaceShuttle, "will take",daysToMoon,"days to reach the Moon");